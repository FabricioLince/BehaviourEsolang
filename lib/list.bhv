  
  
  !&&(
    max = &a > &[(a>b a) b]
    min = &a > &[(a<b a) b]
    insert = &[(i>=#a a+v) (i<=0 {v}*a) (a/i+v)*(a/-(#a-i))]
    split = &[(i<0 {{} a}) (i>#a {a {}}) {a/i a/-(#a-i)}]
    sort = &[
      (#a<2 a)
      (
        pivot = a%0
        c = l = r = {}
        a*&&[(a==pivot c+=a) (a<pivot l+=a) r+=a]
        (sort:l) * c * (sort:r)
      )
    ]
    
    find = &a<b
    any = &~~(a<b)
    all = &~(a<~b)
    count = &#(a/b)
    
    &&(?r=a ?a=() r)
  )